<h1>Pending Trader Sign-ups</h1>
<table>
  <tr>
    <th>Email</th>
    <th>Actions</th>
  </tr>
  <% @pending_users.each do |user| %>
    <tr>
      <td><%= user.email %></td>
      <td>
        <%= link_to 'Approve', approve_admin_user_path(user), method: :patch %>
      </td>
    </tr>
  <% end %>
</table>
